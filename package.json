{
  "name": "binomial-options",
  "version": "2.0.0",
  "description": "Professional vanilla JavaScript library for options pricing using binomial and Black-Scholes models",
  "type": "module",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "exports": {
    ".": {
      "import": "./lib/index.js",
      "types": "./lib/index.d.ts"
    },
    "./core": {
      "import": "./src/core/binomial.js",
      "types": "./src/core/binomial.d.ts"
    },
    "./blackscholes": {
      "import": "./src/core/blackscholes.js",
      "types": "./src/core/blackscholes.d.ts"
    },
    "./trinomial": {
      "import": "./src/core/trinomial.js",
      "types": "./src/core/trinomial.d.ts"
    },
    "./jumpdiffusion": {
      "import": "./src/core/jumpdiffusion.js",
      "types": "./src/core/jumpdiffusion.d.ts"
    },
    "./montecarlo": {
      "import": "./src/core/montecarlo.js",
      "types": "./src/core/montecarlo.d.ts"
    },
    "./option": {
      "import": "./src/models/option.js",
      "types": "./src/models/option.d.ts"
    },
    "./dividends": {
      "import": "./src/utils/dividends.js",
      "types": "./src/utils/dividends.d.ts"
    },
    "./greeks": {
      "import": "./src/utils/greeks.js",
      "types": "./src/utils/greeks.d.ts"
    }
  },
  "files": [
    "lib/",
    "src/",
    "examples/",
    "docs/",
    "README.md",
    "LICENSE",
    "*.d.ts"
  ],
  "scripts": {
    "test": "node tests/run-unit-tests.js",
    "test:legacy": "node tests/test.cjs",
    "test:unit": "node --test --test-reporter=spec tests/unit.test.js",
    "test:coverage": "node --test --experimental-test-coverage tests/unit.test.js",
    "test:all": "npm run test:unit && npm run test:legacy && npm run validate",
    "validate": "node tests/validate-real-market.cjs",
    "example": "node examples/basic-usage.js",
    "docs": "node scripts/generate-docs.js"
  },
  "keywords": [
    "options",
    "derivatives", 
    "finance",
    "binomial",
    "black-scholes",
    "trinomial",
    "jump-diffusion",
    "monte-carlo",
    "options-pricing",
    "greeks",
    "implied-volatility",
    "financial-mathematics",
    "quantitative-finance",
    "vanilla-js",
    "no-dependencies"
  ],
  "author": {
    "name": "Binomial Options Project",
    "email": "contact@binomial-options.dev",
    "url": "https://github.com/binomial-options/js-library"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/binomial-options/js-library.git"
  },
  "bugs": {
    "url": "https://github.com/binomial-options/js-library/issues"
  },
  "homepage": "https://github.com/binomial-options/js-library#readme",
  "engines": {
    "node": ">=14.0.0"
  },
  "browser": "./lib/index.js",
  "directories": {
    "lib": "lib",
    "test": "tests",
    "example": "examples",
    "doc": "docs"
  },
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/binomial-options"
  },
  "config": {
    "defaultParameters": {
      "riskFreeRate": 0.04,
      "dayCount": 252,
      "steps": 50,
      "exerciseStyle": "american"
    }
  },
  "validation": {
    "marketDataDate": "2024-06-24",
    "optionsTested": 671360,
    "avgIVDifference": "5.0%",
    "accuracy": "85% within 5% IV difference",
    "selfConsistency": "0.52% error"
  },
  "devDependencies": {},
  "dependencies": {},
  "peerDependencies": {},
  "optionalDependencies": {}
}
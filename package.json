{
  "name": "options-education-platform",
  "version": "3.0.0",
  "description": "Interactive educational platform for understanding options pricing, Greeks, and algorithms with 5 validated pricing models",
  "type": "module",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "exports": {
    ".": {
      "import": "./lib/index.js",
      "types": "./lib/index.d.ts"
    },
    "./core": {
      "import": "./src/core/binomial.js",
      "types": "./src/core/binomial.d.ts"
    },
    "./blackscholes": {
      "import": "./src/core/blackscholes.js",
      "types": "./src/core/blackscholes.d.ts"
    },
    "./trinomial": {
      "import": "./src/core/trinomial.js",
      "types": "./src/core/trinomial.d.ts"
    },
    "./jumpdiffusion": {
      "import": "./src/core/jumpdiffusion.js",
      "types": "./src/core/jumpdiffusion.d.ts"
    },
    "./montecarlo": {
      "import": "./src/core/montecarlo.js",
      "types": "./src/core/montecarlo.d.ts"
    },
    "./option": {
      "import": "./src/models/option.js",
      "types": "./src/models/option.d.ts"
    },
    "./dividends": {
      "import": "./src/utils/dividends.js",
      "types": "./src/utils/dividends.d.ts"
    },
    "./greeks": {
      "import": "./src/utils/greeks.js",
      "types": "./src/utils/greeks.d.ts"
    }
  },
  "files": [
    "lib/",
    "src/",
    "examples/",
    "docs/",
    "README.md",
    "LICENSE",
    "*.d.ts"
  ],
  "scripts": {
    "dev": "npx http-server . -p 8080 -c-1 --cors",
    "serve": "npx http-server . -p 8080 -c-1 --cors", 
    "start": "npx http-server . -p 8080 -c-1 --cors",
    "test": "node tests/run-unit-tests.js",
    "test:legacy": "node tests/test.cjs",
    "test:unit": "node --test --test-reporter=spec tests/unit.test.js",
    "test:coverage": "node --test --experimental-test-coverage tests/unit.test.js",
    "test:all": "npm run test:unit && npm run test:legacy && npm run validate",
    "validate": "node tests/validate-real-market.cjs",
    "example": "node examples/basic-usage.js"
  },
  "keywords": [
    "options-education",
    "financial-education",
    "interactive-learning",
    "greeks-visualization",
    "options-pricing",
    "quantitative-finance",
    "educational-platform",
    "derivatives",
    "binomial",
    "black-scholes",
    "monte-carlo",
    "data-visualization",
    "financial-mathematics",
    "learning-tools",
    "web-platform"
  ],
  "author": {
    "name": "Options Education Platform",
    "email": "contact@options-education.dev",
    "url": "https://github.com/options-education/platform"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/options-education/platform.git"
  },
  "bugs": {
    "url": "https://github.com/options-education/platform/issues"
  },
  "homepage": "https://[mywebsite.com]/options",
  "engines": {
    "node": ">=14.0.0"
  },
  "browser": "./lib/index.js",
  "directories": {
    "lib": "lib",
    "test": "tests",
    "example": "examples",
    "doc": "docs"
  },
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/binomial-options"
  },
  "config": {
    "defaultParameters": {
      "riskFreeRate": 0.04,
      "dayCount": 252,
      "steps": 50,
      "exerciseStyle": "american"
    }
  },
  "validation": {
    "marketDataDate": "2024-06-24",
    "optionsTested": 671360,
    "avgIVDifference": "5.0%",
    "accuracy": "85% within 5% IV difference",
    "selfConsistency": "0.52% error"
  },
  "devDependencies": {},
  "dependencies": {},
  "peerDependencies": {},
  "optionalDependencies": {}
}